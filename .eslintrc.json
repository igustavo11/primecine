{
  "extends": ["react-app", "react-app/jest"],
  "rules": {
    "no-unused-vars": "off",
    "react-hooks/exhaustive-deps": "off",
    "jsx-a11y/alt-text": "off",
    "react/jsx-key": "off",
    "react/prop-types": "off",
    "import/no-anonymous-default-export": "off",
    "no-console": "off",
    "no-undef": "off",
    "eqeqeq": "off"
  }
}
